{
  "name": "esbuild-plugin-auto-env",
  "version": "0.1.0",
  "description": "An esbuild plugin which automatically replaces environment variables used in code with those defined at build time.",
  "homepage": "https://github.com/harrydowning/esbuild-plugin-auto-env#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/harrydowning/esbuild-plugin-auto-env.git"
  },
  "license": "MIT",
  "author": {
    "name": "Harry Downing",
    "email": "harrydowning.dev@gmail.com"
  },
  "exports": {
    "require": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.cjs"
    },
    "import": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.mjs"
    }
  },
  "main": "dist/plugin.cjs",
  "module": "./dist/plugin.mjs",
  "types": "./dist/plugin.d.ts",
  "scripts": {
    "build": "node build.mjs && tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "esbuild": "^0.23.1",
    "typescript": "^5.5.4"
  }
}
